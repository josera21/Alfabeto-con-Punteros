#include <cstdlib>
#include <iostream>
#include <stdio.h>
using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {

    char *buffer;
    buffer = new char['A'];
    for(*buffer = 'A'; *buffer <= 'Z'; (*buffer)++){
        cout<<"Alfabeto: "<<*buffer<<endl;
	      cout<<"Direccion: "<<buffer<<endl;
    }
    return 0;
}
