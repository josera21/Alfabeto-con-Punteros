#include <iostream>
#include <stdlib.h>
#include <stdio.h>
#include <windows.h>
using namespace std;

int main()
{
	char *buffer;
	buffer = new char['A'];
	
	for(*buffer = 'A'; *buffer <= 'Z'; (*buffer)++){
		cout<<"Alfabeto: "<<(*buffer)<<endl;
		cout<<"Direccion: "<<buffer<<endl;
	}
	return 	0;
}
